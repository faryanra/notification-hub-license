{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.8.0"
    }
  },
  "rewrites": [
    { "source": "/verify", "destination": "/api/verify.php" },
    { "source": "/license/verify", "destination": "/api/verify.php" },
    { "source": "/", "destination": "/public/index.php" },
    { "source": "/status", "destination": "/api/status.php" }
  ]
}
